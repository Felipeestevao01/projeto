{
    "type": "node",
    "request": "launch",
    "name": "LOCAL TESTING",
    "protocol": "inspector",
    "runtimeExecutable": "node",
    "runtimeArgs": [
        "--inspect=9222"
    ],
    "program": "${workspaceFolder}/server.js",
    "restart": true,
    "env": {
        "FOO": "BAR"
    },
    "console": "integratedTerminal",
    "internalConsoleOptions": "neverOpen",
    "skipFiles": [
        "<node_internals>/**"
    ]
}